{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/app.js",
      "destination": "/app.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
